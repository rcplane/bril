@main {
  input: int = const 645634654;
  zero: int = const 0;
  ten: int = const 10;
  tru: bool = const true;
.begin:
  digit: int = call @peel_last_digit input;
  input: int = div input ten;
.check_result:
  br tru .check_done .process_result;
.process_result:
  jmp .check_result;
.check_done:
  done: bool = eq input zero;
  br done .done .begin;
.done:
}
