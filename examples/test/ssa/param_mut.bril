@main(input: int) {
    zero: int = const 0;
    ten: int = const 10;
    result: int = const 0;
 .begin:
    digit: int = const 0;
    input: int = const 0;
    result: int = add result digit;
 .check_result:
    print result;
    processed: bool = const true;
    br processed .check_done .process_result;
 .process_result:
    r0: int = const 0;
    result: int = div result ten;
    result: int = add result r0;
    jmp .check_result;
 .check_done:
    done: bool = eq input zero;
    br done .done .begin;
 .done:
    print result;
}

